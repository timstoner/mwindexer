<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="coordIndexer" class="org.mwindexer.indexer.CoordIndexer">
		<constructor-arg name="coordPattern" value="\{\{Coord.*\}\}" />
		<constructor-arg name="fieldName" value="coord_p" />
	</bean>
	<bean id="categoryIndexer" class="org.mwindexer.indexer.CategoryIndexer">
		<constructor-arg name="pattern" value="\\[\\[Category:.*\\]\\]" />
		<constructor-arg name="fieldName" value="category_ts" />
	</bean>


</beans>
